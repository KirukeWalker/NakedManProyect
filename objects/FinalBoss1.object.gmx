          <argument>
            <kind>1</kind>
            <string>//Gravedad
if(vsp &lt; 10) vsp += grav;
if(place_meeting(x,y+vsp, Terreno))
{
    while(!place_meeting(x, y+sign(vsp), Terreno))
    {
        y+=sign(vsp);
    }
    vsp=0;
}
y += vsp;
//Salto
if(place_meeting(x, y+1, Terreno) &amp;&amp; y &gt; 260)
{
    if(random(60) &lt;= 1)
    { 
        vsp = -jumpspeed;
    }
}
//Controlamos cambio de dirección
change = false;
if(x &gt; object5.x) //Comparamos la posición del boss con la del prota
{ 
    looking_left = true;
    image_xscale = 1;
    interactor.image_xscale = 1;
}
if(x &lt; object5.x)
{
    looking_left = false;
    image_xscale = -1;
    interactor.image_xscale = -1;
}
//Comprobamos posición del prota para el encaramiento
if(x &lt; instance_position(x, y, object5)) 
    looking_left = true;
//Movemos el boss si no ataca
if (sprite_index = spr_boss1_move || sprite_index=spr_boss1_jump)
{
    if(looking_left = true &amp;&amp; !place_meeting(x-1, y, Terreno)) x-=movespeed;
    else if (looking_left = false &amp;&amp; !place_meeting(x+1, y, Terreno)) x+=movespeed;
}
//Movemos el interactor con el boss
interactor.x = x;
interactor.y = y;
//Recibir ataque
if (place_meeting (x,y, obj_cacota)) {
    global.vidaboss -=1;
    instance_deactivate_object(obj_cacota);
}
//Morir del todo
if (global.vidaboss = 0)
{ 
    ini_open("SystemNM.txt")
    ini_key_delete("NhTPahPmx","MeFjgPFREndg")
    ini_write_string("NhTPahPmx","MeFjgPfREndg","uFscFjgWfjE");
    ini_close()
    global.muertes += 1;
    obj_fade.fadeIn = false;
    obj_fade.alarm[1] = room_speed * 0.05;
    instance_destroy();   
}
//Solo si esta en el suelo
if(place_meeting(x, y+1, Terreno))
{
    //Atacar
    if ((distance_to_object (object5) &lt; 20)) 
    {
        sprite_index = spr_boss1_attack;
        interactor.attacking = true;
    }
    else{
        sprite_index = spr_boss1_move;
    }
    //Volver a la normalidad
    if (sprite_index = spr_melee_attack &amp;&amp; image_index = 3){ 
        sprite_index = spr_boss1_move;
        interactor.attacking = false;
    }
}
else  
    sprite_index = spr_boss1_jump;
</string>
