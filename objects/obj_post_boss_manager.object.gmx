            <kind>1</kind>
            <string>//Chequeo para mejores tiempos
lastTime = global.counter + ((30 - global.stepCounter) / 30);
bestTime = global.best_times[global.last_level] + 
    ((30 - global.best_stepTimes[global.last_level]) / 30);
    
if(lastTime &lt; bestTime){
    global.best_times[global.last_level] = global.counter;
    global.best_stepTimes[global.last_level] = global.stepCounter;
    //Y aquí pondremos para desbloqueables
    ini_open("SystemNM.txt");
    switch(global.last_level){
        //ya iremos poniendo los casos
        case 1:
            //ini_write_real(base64_encode("Unlockables"), base64_encode("timeOne"), base64_encode(string(global.counter)));
            //ini_write_real(base64_encode("Unlockables"), base64_encode("stepTimeOne"), base64_encode(string(global.stepCounter)));
            ini_write_real("Unlockables", "timeOne", global.counter);
            ini_write_real("Unlockables", "stepTimeOne", global.stepCounter);
        break;
        case 2:
            ini_write_real("Unlockables", "timeTwo", global.counter);
            ini_write_real("Unlockables", "stepTimeTwo", global.stepCounter);
        break;
        case 3:
            ini_write_real("Unlockables", "timeThree", global.counter);
            ini_write_real("Unlockables", "stepTimeThree", global.stepCounter);
        break;
        case 4:
            ini_write_real("Unlockables", "timeFour", global.counter);
            ini_write_real("Unlockables", "stepTimeFour", global.stepCounter);
        break;
        case 5:
            ini_write_real("Unlockables", "timeFive", global.counter);
            ini_write_real("Unlockables", "stepTimeFive", global.stepCounter);
        break;
    }
    ini_close();
}

//Operacion adicional para el draw
lastStepTime = int64((30 - global.stepCounter) * 3.3);
bestStepTime = int64((30 - global.best_stepTimes[global.last_level]) * 3.3);
</string>
