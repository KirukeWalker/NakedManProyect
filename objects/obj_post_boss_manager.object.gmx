            <string>//Pretar botón para continuar
if(keyboard_check_released(vk_enter) || keyboard_check_released(vk_space) ||
    gamepad_button_check(0, gp_face1)){
    //Reseteamos el timer
    global.counter = 0; 
    switch(global.last_level){
        case 1:
            room_goto(Nivel_Playa1);
        break;
        case 2:
            room_goto(Nivel_Bosque_2);
        break;
        case 3:
            room_goto(Nivel_Nieve);
        break;
        case 4:
            room_goto(Final_Level);
        break;
        case 5:
            room_goto(god_room_01);
        break;
        //Para el boss final ya veremos
        case 6:
            room_goto(Credits);
        break;
    }  
}
