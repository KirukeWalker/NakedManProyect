            <string>//Tecla para avanzar
if (keyboard_check_pressed(vk_enter)) || 
        (keyboard_check_pressed(vk_space)) || 
        (gamepad_button_check_pressed(0,gp_face1) || 
        joystick_check_button(2, 3)) {
    fadeIn = false;
    }
//Tecla para saltar
if (keyboard_check_pressed(vk_escape)) || 
        (gamepad_button_check_pressed(0,gp_start)) {
    room_goto_next();
    }
    
//Fade in y out
if (fadeIn == true)
{
    if (alpha &gt; 0){
        alpha -= fading_speed;
    }
    else{
        alpha = 0;
    }
}
else if (fadeIn = false) {
    if(alpha &lt; 1){
        alpha += fading_speed;
    }
    else{
        alpha = 1;
        //Siguiente cacho de texto
        if(text &lt; 3){
            text ++;
            fadeIn = true;
            alarm_set(0, 240);
        }
        else    //Y siguiente room
            room_goto_next();
    }
}
</string>
