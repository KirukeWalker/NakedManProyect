            <string>//Comprobamos paredes
//Horizontales
if(place_meeting(x + (FinalBoss3.move_speed * h_direction), y, obj_suelo_bosque))
{
    while(!place_meeting(x + h_direction, y, obj_suelo_bosque))
        x += h_direction;
    h_direction *= -1;
    //image_xscale *= -1;
}
//Verticales
if(place_meeting(x, y + (FinalBoss3.move_speed * v_direction), obj_suelo_bosque))
{
    while(!place_meeting(x, y + v_direction, obj_suelo_bosque))
        y += v_direction;
    v_direction *= -1;
    //image_yscale *= -1;
}
//Movimiento
x += FinalBoss3.move_speed * h_direction;
y += FinalBoss3.move_speed * v_direction;

//Chequeo para ver si se destruye (con 20 de vida)
if(global.vidaboss &lt;= qeue_position*2){
    //Destruimos los segmentos previamente creados
    with(obj_unlinked_segment) instance_destroy();
    //Creamos dos segmentos que rebotar√°n a esa altura
    segment1 = instance_create(x, y, obj_unlinked_segment);
    segment2 = instance_create(x, y, obj_unlinked_segment);
    segment1.move_speed = 6;
    segment2.move_speed = 6;
    segment1.h_direction = -h_direction;
    segment1.v_direction = v_direction;
    segment2.h_direction = h_direction;
    segment2.v_direction = -v_direction;
    instance_destroy();
}
</string>
