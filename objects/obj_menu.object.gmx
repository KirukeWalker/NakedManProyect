move2 += max(keyboard_check_pressed(vk_left), keyboard_check_pressed(ord("A")), 0);
move -= max(keyboard_check_pressed(vk_up), keyboard_check_pressed(ord("W")), 0);
move += max(keyboard_check_pressed(vk_down), keyboard_check_pressed(ord("S")), 0);
//Control de joystick
if(joystick_ypos(2) != 0 &amp;&amp; joy_check == true){
    if(joystick_ypos(2) &gt; 0.5){
        move += 1;
    }
    if(joystick_ypos(2) &lt; -0.5){
        move -= 1;
    }
    joy_check = false;
    alarm_set(0, 5);
}
if (gamepad_button_check_pressed(0,gp_padd)) {
    move += 1;
    }
if (gamepad_button_check_pressed(0,gp_padu)) {
    move -= 1;
    }
if(move != 0)
{
    mpos += move;
    if(mpos &lt; 0) mpos = 3;
    if(mpos &gt; 3) mpos = 0;
}
if(move2 != 0)
{
    npos += move2;
    if(npos &lt; 0) npos = 1;
    if(npos &gt; 1) npos = 0;
}

var push;
push = max(keyboard_check_pressed(vk_enter), 0);
if (gamepad_button_check_pressed(0,gp_face1) || joystick_check_button(2, 3)) {
    push += 1;
    }
if(push = 1) scr_menu();
