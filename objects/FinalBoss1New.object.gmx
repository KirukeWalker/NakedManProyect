    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>//Gravedad
if(vspeed &lt;= 10 &amp;&amp; apply_gravity == true){
    grav += 2;
    vspeed += grav;
}

//Comportamiento dependiendo de los estados
switch(status){
    //Check the entrace of the player in the proper room
    case 0:
        if(player.x &gt; 554){
            status = 1;
            health_bar = instance_create(608, 192, obj_healthbar_anal);
            //
            obj_camera2.status = 2;
            obj_camera2.objectiveX = 832;
            obj_camera2.objectiveY = 384;
        }
    break;
    //Close the "door"
    case 1:
        if(block[0].y &lt; 512){
            for(i = 0; i &lt; array_length_1d(block); i++){
                block[i].y += 16;
            }
        }
        else
            status = 2;
    break;
    //Intenta embestir al prota
    case 2:
        if(abs(hspeed) &lt; sprint_speed){
            hspeed += direction_looking;
        }
        if(last_round == false){
            //Segun la vida que pierda pasamos a una fase u otra
            if(global.vidaboss &lt;= 4 &amp;&amp; phase == 2){
                status = 3;
                phase = 3;
            }
            if(global.vidaboss &lt;= 8 &amp;&amp; phase == 1){
                status = 3;
                phase = 2;
            }
            else if(global.vidaboss &lt;= 12 &amp;&amp; phase == 0){
                status = 3;
                phase = 1;
            }
        }
        else{//Para el segundo asalto
            if(global.vidaboss &lt;= 3 &amp;&amp; phase = 6){
                status = 3;
                phase = 7;
            }
            if(global.vidaboss &lt;= 6 &amp;&amp; phase = 5){
                status = 3;
                phase = 6;
            }
            if(global.vidaboss &lt;= 9 &amp;&amp; phase = 4){
                status = 3;
                phase = 5;
            }
        }
    break;
    //Ve a una de las plataforas de los lados y sube
    case 3:
        if(abs(hspeed) &lt; sprint_speed){
            hspeed += direction_looking;
        }
        if(x &lt; 580 || x &gt; 1080){
            x = clamp(x, 580, 1080);
            hspeed = 0;
            direction_looking = sign(800 - x);
            //Que no salte en la parte final
            if(phase &lt; 5)
                vspeed -= 40;
            status = 4;
        }
    break;
    //Shoot some balls to the floor
    case 4:
        if(platform_fire_created == false &amp;&amp; phase != 3 &amp;&amp; last_round == false){
            script_execute(scr_create_flames);
            //
            platform_fire_created = true;
        }
        if(shooting == false){
            apply_gravity = true;
            shooting = true;
            alarm_set(0, time_between_shoots);
        }
    break; 
    //Baja de la plataforma
    case 5:
        if(y &gt; 560){
            status = 2;
            apply_gravity = false;
        }
    break;  
    //Secuencia de muerte
    case 6:
        apply_gravity = true;
        if(abs(hspeed) &lt; sprint_speed/4)
            hspeed += direction_looking;
        if(place_meeting(x, y, Lava))
            hspeed = 0;
        if(y &gt; 700){
            status = 7; 
            obj_fade.fadeIn = false;
            //Para que no finalice la pelea
            global.vidaboss = -1;
        }
    break;
    //"Morir"
    case 7:
        //Fade in de nuevo
        if(obj_fade.alpha == 1){
            global.vidaboss = 1;
            last_round = true;
            obj_fade.fadeIn = true;
            with(Lava) instance_destroy();
            //Crea la piscina de lava
            for(i = 0; i &lt; 18; i++){
                for(j = 0; j &lt; 18; j++){
                     lava[i, j] = instance_create(i * 32 + 544, j * 32 + 640, Lava);
                }
            }
            //Ajsuta las profundidades
            obj_terreno_profundo.depth = -3;
            Lava.depth = -2;
            Terreno.depth = -1;
            depth = -1;
            object5.depth = -1;
            //Resetea la vida del prota
            global.vida = 3;
            lava_reference = instance_find(Lava, 0);
            status = 8;
        }
    break; 
    //Subida de lava y de vida
    case 8:
        //Temblor de cámara
        if(obj_camera2.status != 3){
            obj_camera2.status = 3;
            obj_camera2.alarm[0] = 10;
            obj_camera2.alarm[1] = 5;
        }
        if(lava_reference.y &gt; 480){
            for(i = 0; i &lt; 18; i++){
                for(j = 0; j &lt; 18; j++){
                     lava[i, j].y -= 0.5;
                }
            }
        }
        if(global.vidaboss &lt; obj_healthbar_anal.initialLife)
            global.vidaboss += 0.05;
        if(lava_reference.y == 480 &amp;&amp; global.vidaboss == obj_healthbar_anal.initialLife){
            y = 560;
            x = 768;
            hspeed = 12 * direction_looking;
            status = 3;
            phase = 4;
        }
    break;
    //
}
//Control colisiones vertical (solo hacia abajo)
if(place_meeting(x, y + vspeed, obj_solid_terrain) &amp;&amp; status != 5){
    while(place_meeting(x, y + vspeed, obj_solid_terrain)
        &amp;&amp; vspeed &gt; 0){
        vspeed --;
    }
    grav = 0;
}
//Control colisiones horizontal (cuando embiste)
if(place_meeting(x + hspeed, y, obj_solid_terrain) &amp;&amp; status == 2){
    hspeed = 0;
    direction_looking *= -1;
    //Temblor de cámara
    if(last_round == true){
        obj_camera2.status = 3;
        obj_camera2.alarm[0] = 10;
        obj_camera2.alarm[1] = 5;
    }
}
//Encaramiento
image_xscale = -direction_looking;
//"Morir"
if(global.vidaboss == 0 &amp;&amp; last_round == false){
    status = 6;
    shoot_counter = 0;
    obj_healthbar_anal.initialLife = 12;
    depth = -2;
    with(obj_fire_splash) instance_destroy();
    with(obj_fireball) instance_destroy();
    with(obj_fire_column) instance_destroy();
}
//Morir del todo
if (global.vidaboss == 0 &amp;&amp; last_round == true)
{ 
    ini_open("SystemNM.txt")
    ini_key_delete("NhTPahPmx","MeFjgPFREndg")
    ini_write_string("NhTPahPmx","MeFjgPfREndg","uFscFjgWfjE");
    ini_close()
    global.muertes += 1;
    //obj_fade.fadeIn = false;
    obj_fade_white.fadeIn = false;
    //obj_fade.alarm[1] = room_speed * 0.05;
    instance_destroy();   
}

//Control de sprites
if(hspeed != 0)
    sprite_index = spr_boss1_move;
else if(vspeed != 0)
    sprite_index = spr_boss1_jump;
else
    sprite_index = spr_boss1_idle;
</string>
          </argument>
        </arguments>
      </action>
